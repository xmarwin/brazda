<?xml version="1.0" encoding="utf-8"?>
<gpx xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" creator="BRAZDAteam All Rights Reserved. http://www.brazda-gc.cz" xsi:schemaLocation="http://www.topografix.com/GPX/1/0 http://www.topografix.com/GPX/1/0/gpx.xsd http://www.groundspeak.com/cache/1/0 http://www.groundspeak.com/cache/1/0/cache.xsd" xmlns="http://www.topografix.com/GPX/1/0">
    <name>Stanoviště závodu BRAZDA</name>
    <desc>Seznam stanovišť závodu BRAZDA vygenerovaný z brazda-gc.cz</desc>
    <author>Account "BRAZDAteam" From Geocaching.com</author>
    <email>info@brazda-gc.cz</email>
    <url>http://www.brazda-gc.cz/#/posts</url>
    <urlname>BRAZDA - Brutální Rožnovský Závod Družstev Amatérů</urlname>
    <time>{date('c')}</time>
    <keywords>cache, geocache</keywords>
    <bounds minlat="49.5280778" minlon="17.9448169" maxlat="49.3806722" maxlon="18.3373956" />
    {foreach $posts as $postId => $post}
    <wpt lat="{$post->latitude}" lon="{$post->longitude}">
        <time>{date('c')}</time>
        <name>GCP{$post->post}</name>
        <desc>{$post->name}, {$post->type_name} ({$post->difficulty}/{$post->terrain})</desc>
        <url>https://app.brazda-gc.cz/#/postDetail/{$post->post}</url>
        <urlname>{$post->name}</urlname>
        <sym>{block |strip}{if $post->post_type == \Brazda\Models\Posts::BEGIN}
            Flag, Green
        {elseif $post->post_type == \Brazda\Models\Posts::END}
            Flag, Red
        {elseif $post->post_type == \Brazda\Models\Posts::ACTIVITY}
            Ball Park
        {elseif $post->post_type == \Brazda\Models\Posts::BONUS}
            City (Capitol)
        {elseif $post->post_type == \Brazda\Models\Posts::CIPHER}
            Puzzle Cache
        {elseif $post->post_type == \Brazda\Models\Posts::CACHE}
            Geocache
        {/if}{/block}</sym>
        <type>{block |strip}{if $post->post_type == \Brazda\Models\Posts::ACTIVITY}
            Geocache|Event Cache
        {elseif $post->post_type == \Brazda\Models\Posts::CIPHER}
            Geocache|Unknown Cache
        {elseif $post->post_type == \Brazda\Models\Posts::CACHE}
             || $post->post_type == \Brazda\Models\Posts::BONUS}
            Geocache|{block|strip}{if $post->cache_type == \Brazda\Models\Posts::TRADITIONAL}
            Traditional Cache
            {elseif $post->cache_type == \Brazda\Models\Posts::MULTICACHE}
            Multi-cache
            {elseif $post->cache_type == \Brazda\Models\Posts::MYSTERY}
            Unknown Cache
            {elseif $post->cache_type == \Brazda\Models\Posts::EARTHCACHE}
            Earthcache
            {elseif $post->cache_type == \Brazda\Models\Posts::WHEREIGO}
            Wherigo Cache
            {elseif $post->cache_type == \Brazda\Models\Posts::LETTERBOX}
            Letterbox Hybrid
            {elseif $post->cache_type == \Brazda\Models\Posts::CITO}
            Cache In Trash Out Event
            {/if}{/block}
        {/if}{/block}</type>
        <groundspeak:cache id="{$post->post}" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
            <groundspeak:name>{$post->name}{if !in_array($post->post_type, [ \Brazda\Models\Posts::BEGIN, \Brazda\Models\Posts::END, \Brazda\Models\Posts::ORGANIZATION, \Brazda\Models\Posts::CACHE ])} ({$post->type_name}){/if}</groundspeak:name>
            <groundspeak:placed_by>BRAZDAteam</groundspeak:placed_by>
            <groundspeak:owner>BRAZDAteam</groundspeak:owner>
            <groundspeak:type>{block |strip}{if $post->post_type == \Brazda\Models\Posts::ACTIVITY}
                Event Cache
            {elseif $post->post_type == \Brazda\Models\Posts::CIPHER}
                Unknown Cache
            {elseif $post->post_type == \Brazda\Models\Posts::CACHE}
                {if $post->cache_type == \Brazda\Models\Posts::TRADITIONAL}
                Traditional Cache
                {elseif $post->cache_type == \Brazda\Models\Posts::MULTICACHE}
                Multi-cache
                {elseif $post->cache_type == \Brazda\Models\Posts::MYSTERY}
                Unknown Cache
                {elseif $post->cache_type == \Brazda\Models\Posts::EARTHCACHE}
                Earthcache
                {elseif $post->cache_type == \Brazda\Models\Posts::WHEREIGO}
                Wherigo Cache
                {elseif $post->cache_type == \Brazda\Models\Posts::LETTERBOX}
                Letterbox Hybrid
                {elseif $post->cache_type == \Brazda\Models\Posts::CITO}
                Cache In Trash Out Event
                {/if}
            {elseif $post->post_type == \Brazda\Models\Posts::BONUS}
                Geocache|Unknown Cache
            {/if}{/block}</groundspeak:type>
            <groundspeak:container>{block |strip}{if $post->post_type == \Brazda\Models\Posts::BEGIN}
                Other
            {elseif $post->post_type == \Brazda\Models\Posts::END}
                Other
            {elseif $post->post_type == \Brazda\Models\Posts::ACTIVITY}
                Other
            {elseif $post->post_type == \Brazda\Models\Posts::CIPHER}
                Other
            {elseif $post->post_type == \Brazda\Models\Posts::CACHE
            || $post->post_type == \Brazda\Models\Posts::BONUS}
                {if $post->cache_size == \Brazda\Models\Posts::MICRO}
                Micro
                {elseif $post->cache_size == \Brazda\Models\Posts::SMALL}
                Small
                {elseif $post->cache_size == \Brazda\Models\Posts::REGULAR}
                Regular
                {elseif $post->cache_size == \Brazda\Models\Posts::LARGE}
                Large
                {else}
                Other
                {/if}
            {/if}{/block}</groundspeak:container>
            <groundspeak:difficulty>{$post->difficulty}</groundspeak:difficulty>
            <groundspeak:terrain>{$post->terrain}</groundspeak:terrain>
            <groundspeak:country>Czech Republic</groundspeak:country>
            <groundspeak:state>Zlinsky kraj</groundspeak:state>
            <groundspeak:short_description html="False"></groundspeak:short_description>
            <groundspeak:long_description html="True">{$post->description}</groundspeak:long_description>
            <groundspeak:encoded_hints>{$post->hint}</groundspeak:encoded_hints>
        </groundspeak:cache>
    </wpt>
        {foreach $post->waypoints as $waypoint}
        {if in_array($waypoint->waypoint_visibility, [ \Brazda\Models\Waypoints::VISIBLE, \Brazda\Models\Waypoints::HIDDEN_COORDINATES ])
         && !in_array($post->post_type, [ \Brazda\Models\Posts::BEGIN, \Brazda\Models\Posts::END, \Brazda\Models\Posts::ORGANIZATION ])}
    <wpt lat="{if $waypoint->waypoint_visibility == \Brazda\Models\Waypoints::VISIBLE}{$waypoint->latitude}{/if}" lon="{if $waypoint->waypoint_visibility == \Brazda\Models\Waypoints::VISIBLE}{$waypoint->longitude}{/if}">
        <time>{date('c')}</time>
        <name>W{$waypoint->waypoint}P{$post->post} {$waypoint->name}</name>
        <cmt />
        <desc>{$waypoint->description}</desc>
        <url>https://app.brazda-gc.cz/#/postDetail/{$waypoint->post}</url>
        <urlname>{$waypoint->name}</urlname>
            {if $waypoint->waypoint_type == \Brazda\Models\Waypoints::FINALE}
        <sym>Geocache</sym>
        <type>Geocache</type>
            {elseif $waypoint->waypoint_type == \Brazda\Models\Waypoints::STAGE}
        <sym>Virtual Stage</sym>
        <type>Waypoint|Virtual Stage</type>
            {elseif $waypoint->waypoint_type ==  \Brazda\Models\Waypoints::REFERENCE}
        <sym>Reference Point</sym>
        <type>Waypoint|Reference Point</type>
            {elseif $waypoint->waypoint_type ==  \Brazda\Models\Waypoints::PARKING}
        <sym>Parking Area</sym>
        <type>Waypoint|Parking Area</type>
            {elseif $waypoint->waypoint_type == \Brazda\Models\Waypoints::WAYPOINT}
        <sym>Waypoint</sym>
        <type>Waypoint|Trailhead</type>
            {/if}
    </wpt>
        {/if}
    {/foreach}
{/foreach}
</gpx>
